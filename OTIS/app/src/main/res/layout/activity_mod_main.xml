<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/rootView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/page_bg">

    <com.bocweb.otis.util.ui.TitleView
        android:id="@+id/tv_title"
        style="@style/common_title_style"
        app:title="您的电梯是否" />

    <View style="@style/line_left_34" />

    <RelativeLayout
        android:id="@+id/rl_parent"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/v01"
            style="@style/mod_main_1_text"
            android:layout_marginStart="90dp"
            android:layout_marginTop="110dp"
            android:alpha="0.33"
            android:lineSpacingExtra="-5dp"
            android:text="经常无法使用"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/v02"
            style="@style/mod_main_1_text"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="110dp"
            android:layout_marginEnd="90dp"
            android:alpha="0.22"
            android:text="不满足新的安全标准"
            android:textSize="12sp" />

        <RelativeLayout
            android:id="@+id/v03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="163.5dp"
            android:layout_marginTop="175dp">

            <View
                android:layout_width="66dp"
                android:layout_height="66dp"
                android:layout_centerInParent="true"
                android:alpha="0.3"
                android:background="@drawable/mod_main1_circle_yellow" />

            <TextView
                style="@style/mod_main_1_text"
                android:layout_centerHorizontal="true"
                android:lineSpacingExtra="-7dp"
                android:text="故障频发"
                android:textSize="29sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/v04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="73dp"
            android:layout_marginTop="287dp">

            <View
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_centerInParent="true"
                android:background="@drawable/mod_main1_circle_stroke_yellow" />

            <TextView
                style="@style/mod_main_1_text"
                android:layout_centerHorizontal="true"
                android:text="装潢破损陈旧"
                android:textSize="10sp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/v05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="275dp"
            android:layout_marginEnd="90dp">

            <View
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_centerInParent="true"
                android:alpha="0.2"
                android:background="@drawable/mod_main1_circle_yellow" />

            <TextView
                android:id="@+id/tv_text05"
                style="@style/mod_main_1_text"
                android:layout_centerHorizontal="true"
                android:lineSpacingExtra="-7dp"
                android:text="配件停产"
                android:textSize="25sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/v06"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="132dp"
            android:layout_marginTop="330dp">

            <View
                android:layout_width="62dp"
                android:layout_height="62dp"
                android:layout_centerInParent="true"
                android:alpha="0.3"
                android:background="@drawable/mod_main1_circle_yellow" />

            <TextView
                style="@style/mod_main_1_text"
                android:layout_centerHorizontal="true"
                android:lineSpacingExtra="-7dp"
                android:text="运输能力不足"
                android:textSize="25sp" />
        </RelativeLayout>

        <TextView
            android:id="@+id/v07"
            style="@style/mod_main_1_text"
            android:layout_marginStart="214dp"
            android:layout_marginTop="354dp"
            android:alpha="0.3"
            android:text="费电"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/v08"
            style="@style/mod_main_1_text"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="355dp"
            android:layout_marginEnd="48dp"
            android:alpha="0.3"
            android:text="响应速度缓慢" />

        <TextView
            android:id="@+id/v09"
            style="@style/mod_main_1_text"
            android:layout_marginStart="210dp"
            android:layout_marginTop="427dp"
            android:alpha="0.3"
            android:lineSpacingExtra="-4dp"
            android:text="噪音"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/v10"
            style="@style/mod_main_1_text"
            android:layout_marginStart="81dp"
            android:layout_marginTop="447dp"
            android:alpha="0.2"
            android:text="无法通过安全验收"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/v11"
            style="@style/mod_main_1_text"
            android:layout_marginStart="177dp"
            android:layout_marginTop="523dp"
            android:alpha="0.5"
            android:lineSpacingExtra="-4dp"
            android:text="运行抖动"
            android:textSize="16sp" />

        <RelativeLayout
            android:id="@+id/v12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="271dp"
            android:layout_marginTop="450dp">

            <View
                android:layout_width="34dp"
                android:layout_height="34dp"
                android:layout_centerInParent="true"
                android:background="@drawable/mod_main1_circle_stroke_yellow" />

            <TextView
                style="@style/mod_main_1_text"
                android:layout_centerHorizontal="true"
                android:alpha="0.8"
                android:lineSpacingExtra="-4dp"
                android:text="维护费用增加"
                android:textSize="16sp" />

        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>